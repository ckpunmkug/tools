{
	"manifest_version": 2
	
	,"version": "0.0"
	,"name": "Extension template"
	,"description": "Extension template with basic functionality"
	,"icons": {
		"48": "icons/icon-48.png"
	}
	
	,"browser_action": {
		"default_title": "Template"
		,"default_icon": {
			"16": "icons/icon16.png"
			,"32": "icons/icon32.png"
		}
	}
	
	,"commands": {
		"parser": {
			"description": "Call the parser controll menu"
			,"suggested_key": {
				"default": "Ctrl+Alt+1"
			}
		}
	}	
	
	,"background": {
		"scripts": [
			"parser.js"
			,"background.js"
		]
	}
	
	,"permissions": [
		"*://*/*"
		,"tabs"
		,"scripting"
	]
}

